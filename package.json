{
   "name": "dentistry",
   "version": "1.0.0",
   "description": "Dentistry test markup",
   "author": "Nikita Suchkov",
   "license": "MIT",
   "browserslist": "> 0.25%, last 1 versions, not dead",
   "repository": "https://github.com/suchkovcode/dentistry",
   "homepage": "https://dentistry.suchkov.cc",
   "private": true,
   "scripts": {
      "start": "cross-env NODE_ENV=development webpack serve",
      "build:dev": "cross-env NODE_ENV=development webpack",
      "build:prod": "cross-env NODE_ENV=production webpack",
      "test:check": "prettier --check src/**/* && eslint && stylelint src/**/*.scss",
      "test:fix": "prettier --write . && eslint --fix --color && stylelint src/**/*.scss --fix",
      "docs:build": "jsdoc --configure jsdoc.json --verbose"
   },
   "lint-staged": {
      "*.js": [
         "prettier --write",
         "eslint --cache --fix --color"
      ],
      "*.scss": "stylelint --fix --color"
   },
   "devDependencies": {
      "cross-env": "^7.0.3",
      "husky": "^9.0.11",
      "lint-staged": "^15.0.1",
      "jsdoc": "^4.0.2",
      "clean-jsdoc-theme": "^4.2.13",

      "webpack": "^5.89.0",
      "webpack-cli": "^5.1.4",
      "webpack-dev-server": "^4.15.1",

      "babel-loader": "^9.1.3",
      "css-loader": "^6.8.1",
      "sass-loader": "^13.3.2",
      "style-loader": "^3.3.3",
      "html-loader": "^4.2.0",
      "postcss-loader": "^7.3.3",

      "html-webpack-plugin": "^5.5.3",
      "mini-css-extract-plugin": "^2.7.6",
      "favicons-webpack-plugin": "^6.0.1",
      "webpack-bundle-analyzer": "^4.9.1",
      "duplicate-package-checker-webpack-plugin": "^3.0.0",
      "image-minimizer-webpack-plugin": "^3.8.3",
      "copy-webpack-plugin": "^11.0.0",

      "imagemin": "^8.0.1",
      "imagemin-gifsicle": "^7.0.0",
      "imagemin-mozjpeg": "^10.0.0",
      "imagemin-jpegtran": "^7.0.0",
      "imagemin-optipng": "^8.0.0",
      "imagemin-svgo": "^10.0.1",

      "sass": "^1.69.4",
      "prettier": "^3.0.3",
      "favicons": "^7.1.4",

      "postcss": "^8.4.31",
      "postcss-sort-media-queries": "^5.2.0",
      "postcss-mq-keyframes": "^0.3.0",
      "autoprefixer": "^10.4.16",
      "postcss-100vh-fix": "^1.0.2",
      "postcss-focus": "^7.0.0",
      "@fullhuman/postcss-purgecss": "^5.0.0",

      "@babel/core": "^7.23.2",
      "@babel/cli": "^7.23.0",
      "@babel/preset-env": "^7.23.2",

      "stylelint": "^15.11.0",
      "stylelint-config-prettier-scss": "^1.0.0",
      "stylelint-config-standard-scss": "^11.0.0",
      "stylelint-order": "^6.0.3",

      "eslint": "^8.51.0",
      "eslint-plugin-import": "^2.28.1",
      "eslint-plugin-prettier": "^5.0.1",
      "eslint-config-prettier": "^9.0.0"
   },
   "dependencies": {
      "swiper": "^10.3.1"
   }
}
